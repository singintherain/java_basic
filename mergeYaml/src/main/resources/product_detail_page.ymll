# 商品详情页
product_detail_page:
  # 买家立即购买按钮的元素配置
  buy_now:
    xpath: "//div[@class='submit']/span[1]/input"

  # 加入购物车按钮
  add_to_cart_button:
    xpath: "//div[@class='submit']/span[2]/input"

  # 商品数量增加减少按钮
  amount_controller:
    decrease:
      xpath: "//dl[@class='numb clear']/dd/p/i[1]"
    increase:
      xpath: "//dl[@class='numb clear']/dd/p/i[2]"

